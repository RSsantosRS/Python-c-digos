a=int(input("Digite o tamano do primeiro lado do triângulo: "))
b=int(input("Digite o tamanho do segundo lado do triângulo: "))
c=int(input("Digite o tamanho do terceiro lado do triângulo: "))

if (a==b and b==c):
    print("Seu triangulo é equilátero")
elif (a!=b and b!=c and c!=a):
    print("Seu triângulo é escaleno")
else:
    print("Seu triangulo é isoceles")
